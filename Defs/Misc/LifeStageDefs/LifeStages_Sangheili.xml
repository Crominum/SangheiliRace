<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <LifeStageDef>
    <defName>FirstbornSangheiliStage</defName>
    <label>Firstborn</label>
    <bodySizeFactor>0.2</bodySizeFactor>
    <healthScaleFactor>0.2</healthScaleFactor>
    <hungerRateFactor>0.2</hungerRateFactor>
    <marketValueFactor>0.5</marketValueFactor>
    <statFactors>
      <MoveSpeed>0.2</MoveSpeed>
    </statFactors>
  </LifeStageDef>

  <LifeStageDef>
    <defName>YoungSangheiliStage</defName>
    <label>young</label>
    <bodySizeFactor>0.5</bodySizeFactor>
    <healthScaleFactor>0.5</healthScaleFactor>
    <hungerRateFactor>0.6</hungerRateFactor>
    <marketValueFactor>0.9</marketValueFactor>
    <statFactors>
      <MoveSpeed>0.65</MoveSpeed>
    </statFactors>
  </LifeStageDef>

   <LifeStageDef>
    <defName>TeenSangheiliStage</defName>
    <label>teenage stage</label>
    <visible>false</visible>
    <reproductive>true</reproductive>
  </LifeStageDef>

  <LifeStageDef>
    <defName>AdultSangheiliStage</defName>
    <label>adult stage</label>
    <visible>false</visible>
    <reproductive>true</reproductive>
  </LifeStageDef>

</Defs>