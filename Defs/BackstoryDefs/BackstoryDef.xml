<Defs>
<AlienRace.BackstoryDef>
    <defName>SangheiliOrphan</defName>
    <title>Sangheili Military Orphan</title>
    <titleShort>Orphan</titleShort>
    <baseDescription>[PAWN_nameDef] was like all of [PAWN_possessive] brothers and sisters, forced into the military as a child and abandoned by their parents.</baseDescription>
    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeFemale>Female</bodyTypeFemale>
	<maleCommonality>50</maleCommonality>
	<femaleCommonality>50</femaleCommonality>
    <slot>Childhood</slot>
    <skillGains>
      <li>
        <defName>Melee</defName>
        <amount>2</amount>
      </li>
	  <li>
        <defName>Shooting</defName>
        <amount>4</amount>
      </li>
    </skillGains>
    <spawnCategories>
      <li>SangheiliBackstory</li>
    </spawnCategories>
  </AlienRace.BackstoryDef>

