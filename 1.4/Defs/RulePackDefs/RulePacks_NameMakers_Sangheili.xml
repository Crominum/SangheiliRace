<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerSangheili</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[pack1] [pack2]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>pack1</keyword>
          <path>Sangheili_Names/Sangheili</path>
        </li>
        <li Class="Rule_File">
          <keyword>pack2</keyword>
          <path>Sangheili_Names/Sangheililast</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

